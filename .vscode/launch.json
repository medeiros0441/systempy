{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Depurador do Python: Arquivo Atual",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "type": "node-terminal",
            "name": "Executar Script: start",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}/frontend"
        }
    ]
}